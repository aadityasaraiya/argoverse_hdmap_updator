<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <arg name="filename" default="$(find argoverse_hdmap_updator)/bags/kitti_odometry_sequence_00.bag" />

    <!-- Play rosbag record -->
    <node 
        pkg="rosbag"
        type="play"
        name="player"
        output="screen"
        args="--clock --quiet --rate=1 $(arg filename)" />

</launch>
